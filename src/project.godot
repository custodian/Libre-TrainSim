[application]

config/name="Libre Train Sim"
config/description="Train Simulator"
run/main_scene="res://Data/UI/main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="libre_train_sim"
config/project_settings_override="user://override.cfg"
boot_splash/image="res://Data/Misc/banner/lts_banner_both.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.65098, 0.65098, 0.65098, 1 )
config/icon="res://Data/Misc/logos/lts_logo_16.png"
config/quit_on_go_back=false
config/project_settings_override.editor="user://override_hack.cfg"
version/dirty=false
version/broken=false
version/custom=false
version/commit=""
version/label="v0.9.beta1"
version/diff_ignore="../.github/workflows/project.godot.ignores"

[audio]

default_bus_layout="res://Data/default_bus_layout.tres"

[autoload]

Math="*res://Data/Scripts/Singletons/Math.gd"
Root="*res://Data/Scripts/Singletons/Root.gd"
jConfig="*res://addons/jean28518.jTools/jConfig.gd"
jTools="*res://addons/jean28518.jTools/jTools.gd"
jEssentials="*res://addons/jean28518.jTools/jEssentials/jEssentials.gd"
jAudioManager="*res://addons/jean28518.jTools/jAudioManager/JAudioManager.gd"
jSaveManager="*res://addons/jean28518.jTools/jSaveManager/jSaveManager.gd"
jSettings="*res://addons/jean28518.jTools/jSettings/JSettings.tscn"
ContentLoader="*res://Data/Scripts/Singletons/content_loader.gd"
LoadingScreen="*res://Data/UI/loading_screen.tscn"
InputHelper="*res://Data/Scripts/Singletons/input_helper.gd"
DebugDraw="*res://addons/zylann.debug_draw/debug_draw.gd"
DebugUi="*res://Data/UI/debug_ui.tscn"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/handheld/orientation="sensor_landscape"
window/size/fullscreen.editor=false

[editor]

scene_naming=2

[editor_plugins]

enabled=PoolStringArray( "res://addons/HaSa1002.light-tools/plugin.cfg", "res://addons/hasa1002.buildversion/plugin.cfg", "res://addons/controller_icons/plugin.cfg", "res://addons/libre_trainsim_modding_tools/plugin.cfg" )

[game]

debug/draw_paths=false
debug/draw_paths.release=false
graphics/fog=true
graphics/shadows=true
audio/main_volume=1.0
audio/music_volume=1.0
audio/game_volume=1.0
gameplay/enable_persons=true
gameplay/view_distance=1000
gameplay/sifa_enabled=true
gameplay/pzb_enabled=true
gameplay/chunk_load_distance=2
gameplay/chunk_unload_distance=2
gameplay/load_all_chunks=false
graphics/enable_dynamic_lights=false
audio/music_volume.editor=0.0
debug/editor_save_thumbnails=true
debug/editor_save_thumbnails.release=false
debug/display_chunk=true
debug/display_chunk.release=false

[global]

back=false

